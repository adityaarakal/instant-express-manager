{
  "name": "instant-express-manager",
  "version": "1.0.131",
  "description": "React PWA TypeScript application for Expense Manager with localStorage",
  "private": true,
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install",
    "prepare": "husky",
    "install-protection": "bash scripts/install-git-protection.sh",
    "init-enforcement-lock": "bash scripts/validate-enforcement-lock.sh",
    "unlock-enforcement": "bash scripts/unlock-enforcement.sh",
    "version": "1.0.69",
    "version:set": "bash scripts/version-utils.sh && bash -c 'set_version $1' _",
    "version:major": "bash scripts/bump-version.sh major",
    "version:minor": "bash scripts/bump-version.sh minor",
    "version:patch": "bash scripts/bump-version.sh patch",
    "test:all": "bash scripts/run-all-tests.sh",
    "test:e2e": "cd frontend && npm run test:e2e",
    "test:e2e:all": "cd frontend && npm run test:e2e:all",
    "test:e2e:browsers": "cd frontend && npm run test:e2e:all",
    "test:modules": "bash scripts/run-module-tests.sh",
    "test:modules:all": "bash scripts/run-module-tests.sh --all",
    "test:demo": "bash scripts/demo-tests.sh",
    "test:demo:ui": "bash scripts/demo-tests-ui.sh",
    "test:demo:headed": "bash scripts/demo-tests-headed.sh",
    "test:demo:html": "bash scripts/demo-tests-html.sh",
    "test:demo:report": "bash scripts/demo-tests-show-report.sh",
    "test:tdd": "bash scripts/tdd-check.sh",
    "test:lock": "bash scripts/lock-test.sh",
    "test:unlock": "bash scripts/unlock-test.sh",
    "test:validate-locks": "bash scripts/validate-test-locks.sh",
    "doc:lock": "bash scripts/lock-doc.sh",
    "doc:unlock": "bash scripts/unlock-doc.sh",
    "doc:validate-locks": "bash scripts/validate-doc-locks.sh",
    "lighthouse": "cd frontend && npm run lighthouse",
    "test:deployment": "bash scripts/test-deployment.sh",
    "pre-deploy": "bash scripts/pre-deployment-check.sh",
    "release:check": "bash scripts/check-release-qualification.sh",
    "release:coverage": "bash scripts/check-test-coverage.sh",
    "release:map": "bash scripts/map-locked-test-coverage.sh",
    "release:manage": "bash scripts/manage-release-branch.sh",
    "release:protect": "bash scripts/protect-release-branch.sh"
  },
  "keywords": [
    "react",
    "pwa",
    "typescript",
    "expense-manager"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "husky": "^9.1.7"
  }
}
